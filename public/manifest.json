{
  "manifest_version": 3,
  "name": "Auto Create Tab Group",
  "description": "Auto Create Tab Group",
  "version": "1.0",
  "options_ui": {
    "page": "options.html"
  },
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/vendor.js", "js/content_script.js"]
    }
  ],
  "permissions": ["storage", "tabGroups", "tabs"],
  "host_permissions": ["<all_urls>"]
}
